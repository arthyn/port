{
  "name": "taisho",
  "productName": "taisho",
  "version": "0.2.2",
  "description": "A ship runner and manager for Urbit OS",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:all": "npm run make:mac && npm run make:linux",
    "make:mac": "electron-forge make --platform darwin --arch x64",
    "make:linux": "electron-forge make --platform linux --arch ia32,x64",
    "make:win": "electron-forge make --platform win32 --arch ia32,x64",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Hunter Miller",
    "email": "hunter@hmiller.dev"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/async": "^3.2.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "autoprefixer": "^10.2.4",
    "css-loader": "^4.2.1",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "electron": "11.2.3",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "fs-extra": "^9.1.0",
    "mini-css-extract-plugin": "^1.3.6",
    "node-loader": "^1.0.1",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
