    "electron-root-path": "^1.0.16",
    "electron-squirrel-startup": "^1.0.0",
    "find-process": "^1.4.4",
    "nedb": "^1.8.0",
    "nedb-async": "^0.1.6",
    "node-ipc": "^9.1.3",
    "query-string": "^6.14.0",
